<div class="form-group">
    <label>Damage Roll</label>
    <input type="text"
           name="system.rolls.Normal.damageRoll"
           value="{{system.rolls.Normal.damageRoll}}"
           data-dtype="Dice" />
</div>
<div class="form-group">
    <label>Missile Velocity Value</label>
    <input type="number"
           name="system.MVV"
           value="{{system.MVV}}"
           data-dtype="Number" />
</div>
<div class="form-group">
    <label>MSR</label>
    <input type="number"
           name="system.MSR"
           value="{{system.MSR}}"
           data-dtype="Number" />
</div>
<div class="form-group">
    <label>Number of Missiles</label>
    <input type="text"
           name="system.missileNum"
           value="{{system.missileNum}}"
           data-dtype="Dice" />
</div>
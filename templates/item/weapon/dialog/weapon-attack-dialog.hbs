{{log this}}
<div class="dialog-content">
    <form>
        <div class="form-group">
            <label>Roll Name</label>
            <select class="roll-name-select" name="rollName">
                {{selectOptions rollData labelAttr="label"}}
            </select>
        </div>
        <div class="form-group">
            <label>Weapon Damage Roll</label>
            <input type="text" class="damage-roll-input" name="damageRoll" value="{{rollData.Normal.damageRoll}}"
                disabled="true" data-dtype="Dice" />
        </div>
        <div class="form-group">
            <label>Weapon To Hit Bonus</label>
            <input type="text" class="to-hit-bonus-input" name="toHitBonus" value="{{rollData.Normal.toHitBonus}}"
                disabled="true" data-dtype="Dice" />
        </div>
        <div class="form-group">
            <label>Other To Hit bonuses</label>
            <input type="text" name="othertoHitBonuses" data-dtype="Dice" />
        </div>
        <div class="form-group">
            <label>Other Damage bonuses</label>
            <input type="text" name="otherDamageBonuses" data-dtype="Dice" />
        </div>
    </form>
</div>
<div class="dialog-buttons">
    <button class="dialog-button {{cssClass}}" data-button="cancel">
        Cancel
    </button>
    <button class="dialog-button {{cssClass}}" data-button="normal">
        Roll Attack
    </button>
</div>